{
  "name": "ads-optimizer-backend",
  "version": "1.0.0",
  "description": "Backend para sistema de otimização de campanhas Google Ads Performance Max",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "build": "npm run db:generate && cd ../frontend && npm install && npm run build",
    "build:frontend": "cd ../frontend && npm install && npm run build",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "node prisma/seed.js",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": ["google-ads", "performance-max", "optimization"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "google-ads-api": "^14.1.0",
    "openai": "^4.20.1",
    "socket.io": "^4.7.2",
    "express-validator": "^7.0.1",
    "google-auth-library": "^9.4.1",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "prisma": "^5.7.0"
  }
}
